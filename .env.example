# Database
DB_USER=cicd_user
DB_PASSWORD=secure_password

# LLM Configuration
LLM_PROVIDER=anthropic  # Options: bedrock, anthropic
MODEL_ID=claude-3-5-sonnet-20241022  # For Bedrock

# AWS Bedrock Configuration
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_SESSION_TOKEN=your_session_token_here
AWS_REGION=us-east-1

# Anthropic Configuration (if switching to anthropic provider)
ANTHROPIC_API_KEY=my_dummy_key

# GitLab Integration (Using internal Docker network)
GITLAB_URL=http://gitlab:80
GITLAB_TOKEN=dummy_token

# SonarQube Integration (Using internal Docker network)
SONAR_HOST_URL=http://sonarqube:9000
SONAR_TOKEN=dummy_token

# Application Settings
SESSION_TIMEOUT_HOURS=4
MAX_TOKENS_PER_SESSION=50000
LOG_LEVEL=INFO

# GitLab Webhook Token (optional but recommended for production)
GITLAB_WEBHOOK_TOKEN=your_webhook_secret_here